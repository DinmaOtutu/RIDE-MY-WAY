{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": ["react", "import", "jsx-a11y"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
    },
    "rules": {
      "react/no-unused-state ": 0,
        "jsx-a11y/label-has-for": 0,
        "react/prop-types": 0,
        "no-console": 1,
          "linebreak-style":[
               "error", "windows"
           ],
           "jsx-a11y/anchor-is-valid": [ "error", {
            "components": [ "Link" ],
            "specialLink": [ "to" ]
          }]
        },
        "env": {
            "es6": true,
            "browser": true,
            "node": true
          },
          "globals": {
            "React": true
          }
}
